umount /target/boot
umount /target/home
umount /target/usr
umount /target/var
umount /target

swapoff /dev/mapper/testvg-lv0_crypt
cryptsetup close "testvg-lv0_crypt"
cryptsetup close "vdd_crypt"

cryptsetup close "md0_crypt"
mdadm --stop /dev/md0

cryptsetup close "testvg-lv1_crypt"
lvm vgchange -an "testvg"

